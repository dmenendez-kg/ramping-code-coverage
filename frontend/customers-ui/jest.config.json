{
  "setupFilesAfterEnv": [
    "<rootDir>/src/test/setupTests.js"
  ],
  "testEnvironment": "jest-environment-jsdom",
  "moduleNameMapper": {
    "\\.(gif|ttf|eot|svg|png)$": "<rootDir>/src/test/fileMock.js",
    "\\.(css|less)$": "identity-obj-proxy"
  },
  "coverageReporters": [
    "json-summary",
    "text",
    "lcov"
  ],
  "coverageThreshold": {
    "global": {
      "lines": 40,
      "statements": 20,
      "functions": 20,
      "branches": 20
    }
  }
}
